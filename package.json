{
  "name": "WeebsLibraryKeeper",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "debug": "~4.1.1",
    "express": "^4.16.4",
    "fs-extra": "8.1.0",
    "is_js": "^0.9.0",
    "lowdb": "^1.0.0",
    "morgan": "^1.9.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "^3.0.1",
    "shortid": "^2.2.15"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10"
  },
  "nodemonConfig": {
    "ignore": [
      "db.json"
    ]
  },
  "scripts": {
